<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"] - WhereAreYou</title>
    <meta charset="utf-8" />

    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,900" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" />
</head>

<body class="container">
    @RenderBody()

    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    <script src="~/js/init.js" type="text/javascript"></script>

    <script src="~/js/entity/CreateRoomViewModel.js" type="text/javascript"></script>
    <script src="~/js/entity/User.js" type="text/javascript"></script>
    <script src="~/js/entity/Location.js" type="text/javascript"></script>
    <script src="~/js/entity/Position.js" type="text/javascript"></script>
    <script src="~/js/entity/Room.js" type="text/javascript"></script>

    <script src="~/js/services/Client.js" type="text/javascript"></script>
    <script src="~/js/services/RoomApiClient.js" type="text/javascript"></script>
    <script src="~/js/services/SsoClient.js" type="text/javascript"></script>
    <script src="~/js/services/ViewEngine.js" type="text/javascript"></script>

    <script src="~/js/models/ApplicationSettings.js" type="text/javascript"></script>
    <script src="~/js/models/UserSettings.js" type="text/javascript"></script>
    <script src="~/js/models/RoomData.js" type="text/javascript"></script>

    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>


    <script type="text/javascript">
        document.applicationSettings = new way.models.applicationSettings();
        document.applicationSettings.apiUrl = '@WhereAreYou.Core.Configuration.Constants.ROOM_API_URL';
        document.applicationSettings.ssoUrl = '@WhereAreYou.Core.Configuration.Constants.SSO_API_URL';
    </script>

    <script src="~/js/primary.js" type="text/javascript"></script>

</body>

</html>
